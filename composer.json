{
    "name": "bednic/json-api",
    "type": "library",
    "description": "JSON API implementation driven by annotations",
    "homepage": "https://gitlab.com/bednic/json-api",
    "keywords": [
        "php",
        "json",
        "api",
        "rest",
        "annotations"
    ],
    "license": "MIT",
    "authors": [
        {"name": "Tomáš Benedikt", "email": "tomas.benedikt@gmail.com"}
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "doctrine/annotations": "^1.6",
        "doctrine/cache": "^1.8",
        "psr/log": "^1.1",
        "psr/http-server-middleware": "^1.0",
        "doctrine/common": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "minimum-stability": "stable",
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "JSONAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\JSONAPI\\": "test/"
        }
    },
    "archive": {
        "exclude": [
            "/docker",
            "/test",
            "/vendor",
            ".*",
            "*.xml",
            "phpDocumentor.phar",
            "phpDocumentor.phar.pubkey"
        ]
    }
}
